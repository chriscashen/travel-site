ts/\" as *;\n// --------------------------\n// DESINATION CARD\n// --------------------------\n.desination__card {\n\n    display: inline-block;\n    width: 28rem;\n    min-height: 40rem;\n    border-radius: 5px;\n    background-color: aqua;\n    position: relative;\n    padding: $padding;\n    background-size: cover;\n    background-position: center;\n    backdrop-filter: blur(5px);\n\n    &:nth-child(1) {\n        background-image: url(\"src/\");\n    }\n\n    &:nth-child(2) {\n        background-image: url(\"../../assets/images/iceland.jpg\");\n\n    }\n\n    &:nth-child(3) {\n        background-image: url(\"../../assets/images/hawaii.jpg\");\n    }\n\n\n    &-title {\n        padding: $padding;\n        position: absolute;\n        color: white;\n        bottom: 0;\n        right: 0;\n\n    }\n}","scss/components/_icon.scss":"@use \"../abstracts/\" as *;\n\n.icon {\n    height: 2.4rem;\n    margin-top: $margin;\n    margin-right: $margin;\n}","scss/layout/_nav.scss":"@use \"../abstracts\" as *;\n\n.nav {\n    display: flex;\n    justify-content: space-between;\n    align-items: center;\n    height: 5.5rem;\n    max-width: 120rem;\n    padding: 1rem;\n    margin: 0 auto;\n\n    &__list{\n        display: flex;\n\n        &-item-link {\n            padding: 1rem;\n            font-size: 1.3rem;\n        }\n    }\n}","scss/layout/_footer.scss":"@use \"../abstracts/\" as *;\n// --------------------------\n// FOOTER\n// --------------------------\n.footer {\n    background-color: $clr-bg-primary;\n    padding: $padding;\n\n    &__content {\n        padding: $padding;\n    }\n\n\n    &__column-item {\n        margin: $margin 0;\n        margin-right: $margin;\n\n        &-sub {\n            color: #DB7A4E\n        }\n\n        &-detail {\n            margin-top: $margin-sm;\n        }\n    }\n}","scss/layout/_hero.scss":"@use \"../abstracts\" as *;\n// --------------------------\n// HERO\n// --------------------------\n.hero {\n    max-width: 110rem;\n    height: 60vh;\n    margin: 0 auto;\n    padding: $padding;\n    display: grid;\n    grid-template-columns: 1fr .75fr;\n    align-items: center;\n    column-gap: 6rem;\n\n    &__left {\n        display: flex;\n        flex-direction: column;\n        align-items: flex-start;\n        gap: 2rem;\n    }\n\n\n    &__image {\n        width: 100%;\n    }\n}","scss/layout/_grid.scss":"@use \"../abstracts/\" as *;\n// --------------------------\n// GRID\n// --------------------------\n.grid-col-3 {\n    display: grid;\n    grid-template-columns: repeat(3, auto);\n    margin: 0 auto;\n    max-width: 95rem;\n    \n    &--space-between {\n        justify-content: space-between;\n    }\n}","scss/layout/_section.scss":"@use \"../abstracts/\" as *;\n// --------------------------\n// SECTION\n// --------------------------\n.section {\n    &--color-tan {\n        background-color: $clr-bg-primary;\n    }\n\n    &__container {\n        max-width: 110rem;\n        margin: $margin-lg auto;\n        padding: $padding;\n    }\n\n    &__content {\n        padding: $padding;\n    }\n}","scss/pages/_destination.scss":".destination {\n    height: 100vh;\n    width: 100vw;\n    &__cover {\n        height: 100%;\n        width: 100%;\n        background-image: url(\"../../assets/images/greece.jpg\");\n        background-color: aqua;\n        object-fit: cover;\n    }\n}"},"lineCount":null}},"error":null,"hash":"df092bd408013c57cb4d723e565b0ef5","cacheData":{"env":{}}}